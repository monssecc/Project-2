generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model profile {
  id             String    @id @default(auto()) @map("_id") @db.ObjectId
  name           String 
  program        String
  year           String?
  campus         String?
  bio            String?
  motivation     String?
  skillsYouHave  String
  skillYouWant   String
  contact        String
  
}
model recipes {
  id             String    @id @default(auto()) @map("_id") @db.ObjectId
  name           String    
  country        String?
  
}

model cats {
  id             String    @id @default(auto()) @map("_id") @db.ObjectId
  adoptionDate   DateTime? @db.Date
  birthDate      DateTime? @db.Date
  breed          String?
  declawed       Boolean?
  description    String?
  fosterDate     DateTime? @db.Date
  fosterDuration Int?
  location       String?
  name           String?
  neutered       Boolean?
  sex            String?
  primaryColor   String?
  secondaryColor String?
  playfulness    Int?
  appetite       Int?
  microchip      Int?
  isAdopted      Boolean?
  food           String?
}

